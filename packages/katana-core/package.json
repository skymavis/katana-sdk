{
  "name": "@sky-mavis/katana-core",
  "version": "0.0.1-alpha",
  "description": "Katana Core SDK",
  "main": "dist/cjs/index.js",
  "module": "dist/mjs/index.js",
  "types": "./dist/types/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/mjs/index.js",
      "require": "./dist/cjs/index.js",
      "types": "./dist/types/index.d.ts"
    },
    "./dist/*": "./dist/*"
  },
  "scripts": {
    "build": "rm -rf dist && rollup -c",
    "lint": "eslint . --ext .js,.ts",
    "lint:fix": "eslint . --ext .js,.ts --fix",
    "clean": "rm -rf .turbo && rm -rf node_modules && rm -rf dist"
  },
  "files": [
    "dist"
  ],
  "sideEffects": false,
  "repository": {
    "type": "git",
    "url": "git@github.com:skymavis/katana-sdk.git"
  },
  "keywords": [    
    "Ronin",
    "Katana",
    "Ronin Dex",
    "Ronin Network",
    "Katana Core SDK",
    "Katana Swap"
  ],
  "author": "SkyMavis",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/skymavis/katana-sdk/issues"
  },
  "homepage": "https://github.com/skymavis/katana-sdk",
  "devDependencies": {
    "typescript": "5.6.2",
    "@rollup/plugin-babel": "6.0.4",
    "@rollup/plugin-json": "6.1.0",
    "rollup": "3.29.4",
    "rollup-plugin-typescript2": "0.36.0",
    "@rollup/plugin-commonjs": "24.0.1",
    "@rollup/plugin-multi-entry": "6.0.0",
    "@rollup/plugin-node-resolve": "15.0.1",
    "rollup-plugin-node-externals": "6.1.1",
    "rollup-plugin-peer-deps-external": "2.2.4",
    "babel-plugin-macros": "3.1.0",
    "babel-plugin-annotate-pure-calls": "0.4.0",
    "babel-plugin-dev-expression": "0.2.3",
    "babel-plugin-polyfill-regenerator": "0.4.1",
    "@babel/plugin-proposal-class-properties": "7.18.6",
    "@babel/preset-env": "7.20.2"
  },
  "dependencies": {
    "@uniswap/sdk-core": "5.3.0",
    "@uniswap/v3-sdk": "3.13.0",
    "@uniswap/v2-sdk": "4.3.2",
    "@ethersproject/address": "5.0.2",
    "@ethersproject/solidity": "5.0.9",
    "tiny-invariant": "1.1.0"
  }
}